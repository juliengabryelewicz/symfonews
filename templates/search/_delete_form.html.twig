<form method="post" class="d-inline-block" action="{{ path('app_search_delete', {'id': search.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ search.id) }}">
    <button class="btn btn-danger">Delete</button>
</form>
